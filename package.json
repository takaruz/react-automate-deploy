{
  "name": "material-webpack-template",
  "version": "0.0.1",
  "description": "ReactJS - Material-UI - Template",
  "scripts": {
    "start": "npm-run-all --parallel start-api start-webpack",
    "start-webpack": "webpack-dev-server --config webpack-dev-server.config.js --hot --inline",
    "start-api": "json-server --watch api/db.json --routes api/routes.json --port 5000",
    "build": "webpack --config webpack-production.config.js --progress --colors"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.25.0",
    "json-server": "^0.8.21",
    "node-sass": "^3.10.0",
    "npm-run-all": "^3.1.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.7",
    "webpack-dev-server": "^2.0.0-beta"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "firebase": "^3.4.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.15.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-motion": "^0.4.5",
    "react-router": "^2.8.1",
    "react-swipeable-views": "^0.7.9",
    "react-tap-event-plugin": "^1.0.0"
  },
  "engines": {
    "node": "6.0.0",
    "npm": "3.8.6"
  }
}
